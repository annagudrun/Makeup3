@model makeup1.ViewModels.NewsFeedViewModel

@{
    ViewBag.Title = "Index";
}

<h2>Fréttaveita</h2>

<div>
    @foreach (var p in Model.photo)
    {
       <div class="myprofile">

         <div class="displaypic">
            <img src="@Url.Content(p.photoUrl)" alt="IMAGES" />
         </div>
        
        <div class="displaytext">
            <p class="text">@p.Caption</p>
            @foreach(var h in p.Hashtags)
            {
                <span>#@h.HastagName</span>
            }
            @*<div style="display:block; background: url(@p); background-size: cover; background-repeat no-repeat; background-position 50% 50%; max-width 100%; height 200px;" class="photo-wrapper"></div>*@
        </div>

       </div>
    }
</div>